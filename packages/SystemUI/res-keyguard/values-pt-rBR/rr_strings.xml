<resources xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- Time displayed on typographic clock face, which displays the time in words.
             Example:
                 It's
                 Four
                 Twenty
                 Nine
         This string requires two arguments: the first in the hours of the time and
         the second is the minutes of the time. The hours string is obtained from
         string-array type_clock_hours below and the minutes string is obtained
         from string-array type_clock_minutes below.
    [CHAR LIMIT=8] -->
    <plurals name="type_clock_header">
        <item quantity="one"><annotation name="color">São</annotation>\n^1\n^2</item>
        <item quantity="few"><annotation name="color">São</annotation>\n^1\n^2</item>
        <item quantity="other"><annotation name="color">São</annotation>\n^1\n^2</item>
    </plurals>
    <!-- Hour displayed in words on the typographic clock face. [CHAR LIMIT=12] -->
    <string-array name="type_clock_hours">
        <item>Doze</item>
        <item>Uma</item>
        <item>Duas</item>
        <item>Três</item>
        <item>Quatro</item>
        <item>Cinco</item>
        <item>Seis</item>
        <item>Sete</item>
        <item>Oito</item>
        <item>Nove</item>
        <item>Dez</item>
        <item>Onze</item>
    </string-array>

